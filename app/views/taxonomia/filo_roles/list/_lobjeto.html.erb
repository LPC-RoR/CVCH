<div style="padding-left: 1em;">
	<small>
	<%= objeto.filo_interaccion.id %>
	<b><%=objeto.filo_rol %></b> : 
	<%= objeto.filo_especie.filo_especie %>
	
	<i class="bi bi-arrow-<%= objeto.primer_rol? ? 'right' : 'left' %>"></i>
	<b><%= objeto.filo_interaccion.filo_roles.where.not(filo_rol: objeto.filo_rol).first.filo_rol %></b> : 
	<%= render layout: '0p/layout/tooltip_link', locals: {link: "/publicos/especies?indice=#{objeto.filo_interaccion.filo_roles.where.not(filo_rol: objeto.filo_rol).first.filo_especie.id}", tooltip: 'Ver Especie'} do %>
		<%= objeto.otro_rol.filo_especie.filo_especie %>
	<% end %>

	<%= render layout: '0p/collapse/link', locals: {label: 'Origen', objeto: objeto, tooltip: 'Referencia'} do %>
		<i class="bi bi-blockquote-left"></i>
	<% end %>
	<%= render layout: '0p/layout/tooltip_link', locals: {link: "/publicos/publicacion/pid=#{objeto.filo_interaccion.publicacion.id}", tooltip: 'Ver publicaciÃ³n'} do %>
		<%= render partial: '0p/general/gly', locals: {gly: 'file'} %>
	<% end %>
	</small>
</div>
<%= render layout: '0p/collapse/display', locals: {label: 'Origen', objeto: objeto} do %>
	<div style="padding-left: 3em;">
		<small><small class="text-muted">
			<%= objeto.filo_interaccion.publicacion.d_quote %>
		</small></small>
	</div>
<% end %>