<p id="notice"><%= notice %></p>

<h1>Filo Esp Cones</h1>

<table>
  <thead>
    <tr>
      <th>Filo especie</th>
      <th>Filo categoria conservacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filo_esp_cones.each do |filo_esp_con| %>
      <tr>
        <td><%= filo_esp_con.filo_especie_id %></td>
        <td><%= filo_esp_con.filo_categoria_conservacion_id %></td>
        <td><%= link_to 'Show', filo_esp_con %></td>
        <td><%= link_to 'Edit', edit_filo_esp_con_path(filo_esp_con) %></td>
        <td><%= link_to 'Destroy', filo_esp_con, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filo Esp Con', new_filo_esp_con_path %>
