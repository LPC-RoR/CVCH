<p id="notice"><%= notice %></p>

<h1>Filo Conflictos</h1>

<table>
  <thead>
    <tr>
      <th>App perfil</th>
      <th>Filo conflicto</th>
      <th>Detalle</th>
      <th>Resuelto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filo_conflictos.each do |filo_conflicto| %>
      <tr>
        <td><%= filo_conflicto.app_perfil_id %></td>
        <td><%= filo_conflicto.filo_conflicto %></td>
        <td><%= filo_conflicto.detalle %></td>
        <td><%= filo_conflicto.resuelto %></td>
        <td><%= link_to 'Show', filo_conflicto %></td>
        <td><%= link_to 'Edit', edit_filo_conflicto_path(filo_conflicto) %></td>
        <td><%= link_to 'Destroy', filo_conflicto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filo Conflicto', new_filo_conflicto_path %>
