	<% end %>

	<!-- MUEVE ELEMENTO -->
	<% if dog? %>
		<%= render layout: '0p/collapse/link', locals: {label: 'MueveElemento', objeto: nil, tooltip: 'Mueve elemento'} do %>
			<i class="bi bi-diagram-2"></i>
		<% end %>
	<% end %>

	<i class="bi bi-dot"></i>

	<small><%= render partial: '0p/tabla/links_e', locals: {objeto: objeto} %></small>
<% end %>