<div class='row justify-content-center'>
	<div class='col-md-4'>
		<%= render layout: '0p/collapse/display', locals: {label: 'MueveEspecie', objeto: nil} do %>
			<%= render partial: 'taxonomia/filo_especies/taxonomia/mueve_especie', locals: {objeto: objeto} %>
		<% end %>

		<%= render layout: '0p/collapse/display', locals: {label: 'TiposEspecie', objeto: nil} do %>
			<%= render partial: 'taxonomia/filo_especies/taxonomia/set_tipo_especie', locals: {objeto: objeto} %>
		<% end %>

		<%= render layout: '0p/collapse/display', locals: {label: 'CategoriasConservacion', objeto: nil} do %>
			<%= render partial: 'taxonomia/filo_especies/taxonomia/set_categoria', locals: {objeto: objeto} %>
		<% end %>

		<% if objeto.parent.blank? %>
			<%= render layout: '0p/collapse/display', locals: {label: 'NuevaSubEspecie', objeto: nil} do %>
				<%= render partial: 'taxonomia/filo_especies/partials/form_nueva_especie', locals: {objeto: objeto} %>
			<% end %>
		<% end %>
	</div>
</div>