<div class="titulo">
	<div class=" row text-info">
		<div class="col-10">
			<i class="bi bi-<%=controller_icon[c]%>"></i> 
			<%=title%>
		</div>
		<div class="col-2" align='right'>
			<% unless link_new.blank? %>
				<%= link_to link_new do %>
					<i class="bi bi-plus-circle"></i>
				<% end %>
			<% end %>
		</div>
	</div>
</div>