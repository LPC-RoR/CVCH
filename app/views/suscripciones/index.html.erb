<p id="notice"><%= notice %></p>

<h1>Suscripciones</h1>

<table>
  <thead>
    <tr>
      <th>Categoria</th>
      <th>Perfil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suscripciones.each do |suscripcion| %>
      <tr>
        <td><%= suscripcion.categoria_id %></td>
        <td><%= suscripcion.perfil_id %></td>
        <td><%= link_to 'Show', suscripcion %></td>
        <td><%= link_to 'Edit', edit_suscripcion_path(suscripcion) %></td>
        <td><%= link_to 'Destroy', suscripcion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Suscripcion', new_suscripcion_path %>
