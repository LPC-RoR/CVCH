<p id="notice"><%= notice %></p>

<h1>Filo Elementos</h1>

<table>
  <thead>
    <tr>
      <th>Filo elemento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filo_elementos.each do |filo_elemento| %>
      <tr>
        <td><%= filo_elemento.filo_elemento %></td>
        <td><%= link_to 'Show', filo_elemento %></td>
        <td><%= link_to 'Edit', edit_filo_elemento_path(filo_elemento) %></td>
        <td><%= link_to 'Destroy', filo_elemento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filo Elemento', new_filo_elemento_path %>
