<p id="notice"><%= notice %></p>

<h1>Ind Modelos</h1>

<table>
  <thead>
    <tr>
      <th>Ind modelo</th>
      <th>Campos</th>
      <th>Ind estructura</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ind_modelos.each do |ind_modelo| %>
      <tr>
        <td><%= ind_modelo.ind_modelo %></td>
        <td><%= ind_modelo.campos %></td>
        <td><%= ind_modelo.ind_estructura_id %></td>
        <td><%= link_to 'Show', ind_modelo %></td>
        <td><%= link_to 'Edit', edit_ind_modelo_path(ind_modelo) %></td>
        <td><%= link_to 'Destroy', ind_modelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ind Modelo', new_ind_modelo_path %>
