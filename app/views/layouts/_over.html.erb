<% if lyt_o_menu %>
	<!-- OVER MENU -->
	<div class="container-fluid">
		<% if ( partial?('0capitan', 'app/over', 'menu_over') and true  )%>
			<div class="menu_over" style="padding-left: <%=lyt_o_menu_padd%>%; padding-right: <%=lyt_o_menu_padd%>%; margin-top: 2px; margin-bottom: 4px;">
				<div class="menu-over-box">
					<div class="menu-over-items">
						<%= render partial: '0capitan/app/over/menu_over' %>
					</div>
				</div>
			</div>
		<% end %>
	</div>
<% end %>

<% if lyt_o_bann %>
	<!-- BANNER -->
	<div class="container-fluid">
		<% if ( partial?('0capitan', 'app/over', 'banner') and true ) %>
			<% unless no_banner_display? %>
				<div class="banner" style="padding-left: <%=lyt_o_bann_padd%>%; padding-right: <%=lyt_o_bann_padd%>%; margin-bottom: 8px;">
					<div class="banner-box">
						<%= render partial: '0capitan/app/over/banner' %>
					</div>
				</div>
			<% end %>
		<% end %>
	</div>
<% end %>