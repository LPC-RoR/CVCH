<%  if @objeto.estado == 'publicada' %>
	<% if false %>
	<% Publicacion::EVALUACION.keys.each do |item| %>
		<div class = 'row'>
			<div class='col-md-1'></div>
			<div class='col-md-3'>
				<small><strong><%= item %></strong></small>
			</div>
			<div class='col-md-3'>
				<small><%= get_evaluacion_publicacion(o, item) %></small>
			</div>
			<div class='col-md-4'>
				<small>
				<% get_btns_evaluacion(o, item).each do |btn| %>
					<%= render partial: '0p/tabla/btn', locals: {texto: "#{btn}", link: "/publicaciones/cambia_evaluacion?publicacion_id=#{o.id}&item=#{item}&evaluacion=#{btn}", delete: false }  %>
				<% end %>
				</small>
			</div>
			<div class='col-md-1'></div>
		</div>
	<% end %>
	<% end %>
<% else %>	
	<% unless @objeto.estado == 'papelera' %>
	<% @menu_areas.each do |area| %>
		<%= render partial: '0p/tabla/btn', locals: {texto: "#{area.area}", link: "/publicaciones/cambia_area?publicacion_id=#{o.id}&area_id=#{area.id}", delete: false }  %>
	<% end %>
	</br></br>
	<% (Publicacion::DOC_TYPES - [@objeto.doc_type]).each do |doc_type| %>
		<%= render partial: '0p/tabla/btn', locals: {texto: "#{doc_type}", link: "/publicaciones/cambia_tipo?publicacion_id=#{o.id}&doc_type=#{doc_type}", delete: false }  %>
	<% end %>
	<% end %>
<% end %>

<div class='row'>
	<div class='col-md-2'>
		<% if @objeto.estado == 'publicada' %>
			<%= render partial: '0p/list/o_links', locals: {titulo: 'Carpetas Actuales', coleccion: @carpetas_actuales, d_field: 'carpeta', o_id: 'carpeta_id', sel: 'no-sel', c: controller_name, a: 'cambia_carpeta', options: {publicacion_id: @objeto.id ,origen: 'actuales'}} %>
		<% end %>
	</div>
	<div class='col-md-2'>
		<% if @objeto.estado == 'publicada' %>
			<%= render partial: '0p/list/o_links', locals: {titulo: 'Destinos', coleccion: @carpetas_destino, d_field: 'carpeta', o_id: 'carpeta_id', sel: 'no-sel', c: controller_name, a: 'cambia_carpeta', options: {publicacion_id: @objeto.id ,origen: 'destinos'}} %>
		<% end %>
	</div>
	<div class='col-md-8'>
		<h6><%= "| #{@objeto.doc_type} | #{@objeto.estado} | #{@objeto.areas.first.area unless @objeto.areas.empty?} |" %></h6>
		<h6><%= @objeto.m_quote %></h6>

		<% unless @objeto.doi.blank? %>
			<%= link_to "https://doi.org/#{@objeto.doi}", "https://doi.org/#{@objeto.doi}", target: :blank %>
		<% end %>

		<% if @objeto.estado == 'duplicado' and @duplicados.any? %>
			<% @duplicados.each do |duplicado| %>
				<h6><%= "| "%><%=link_to duplicado.doc_type, duplicado%><%=" | #{duplicado.estado} | #{duplicado.areas.first.area unless duplicado.areas.empty?} |" %></h6>
				<h6><%= duplicado.m_quote %></h6>
			<% end %>
		<% end %>

	</div>
</div>
</br></br>
