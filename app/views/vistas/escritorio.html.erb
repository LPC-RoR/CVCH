<div class='row'>
	<div class='col-md-3'>
		<%= render partial: '0p/list/selector', locals: {titulo: 'Carpetas', list: @list_selector, sel: @sel, options: @options} %>
	</div>
	<div class='col-md-9'>
		<%= render partial: '0p/tabs/tabs', locals: {tabs: @tabs, tag: 'tab', active: @tab} %>
		<% if @tab == 'Publicaciones' %>
			<%= render partial: '0p/tabla/solo_tabla', locals: {c: 'publicaciones', a: 'index'} %>
		<% else %>

			<%#= render partial: '0p/general/titulo', locals: {c: controller_name, a: nil, titulo: 'BibliografÃ­a', size: '5'} %>

			<div class='row'>
				<div class='col-md-1'>
				</div>
				<div class='col-md-10'>
					<% @coleccion['citas'].each do |cita| %>
						<p><small><%= sanitize cita.m_quote %></small></p>
					<% end %>
				</div>
				<div class='col-md-1'>
				</div>
			</div>

		<% end %>
	</div>
</div>
