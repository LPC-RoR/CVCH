
<%= render partial: '0p/tabs/tabs', locals: { tabs: @tabs, options: @options, token: :tab } %>
<% if @options[:tab] == 'Etiquetas por clasificar' %>
	<h5 class="text-info"><i class="bi bi-tag"></i> Etiquetas por clasificar</h5>
	<%= render partial: '0p/tabla/tabla', locals: {c: 'huerfanas-especies'} %>
<% elsif @options[:tab] == 'Etiquetas de sinónimos' %>
	<h5 class="text-info"><i class="bi bi-tag"></i> Etiquetas de sinónimos</h5>
	<%= render partial: '0p/tabla/tabla', locals: {c: 'sinonimos-especies'} %>
<% end %>