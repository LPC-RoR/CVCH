<!-- FORMULARIO PARA NUEVO ELEMENTO HIJO -->
<small><b class='text-info'>Nuevo sinónimo</b></small>
<%= form_for :nuevo_sinonimo, url: "/filo_especies/#{objeto.id}/agrega_sinonimo" do |form| %>

	<%= render partial: '0p/form_fields/label', locals: { form: form, label: 'Nuevo sinónimo', v: true } %>
	<%= render partial: '0p/form_fields/text', locals: { form: form, token: :filo_sinonimo } %>
	<!--/br-->
	<button type="submit" class="btn btn-info">
	  <i class="bi bi-plus-circle icon-white"></i>
	  <i class="bi bi-tag"></i>
	</button>
<% end  %>
