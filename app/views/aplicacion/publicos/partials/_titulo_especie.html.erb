<h5>
	<i class="bi bi-bug text-<%= params[:indice].blank? ? 'info' : (objeto.revisar ? 'danger' : (objeto.mma_ok ? 'succes' : 'dark')) %>"></i>
	<span class='text-info'>
		<%= objeto.parent.blank? ? 'Especie' : 'Sub especie' %> | 
		<%= "#{objeto.filo_especie} ( #{objeto.referencia} )" %>
	</span>
</h5>
