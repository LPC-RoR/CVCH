<% unless objeto.orden == 1 %>
	<%= link_to "/#{objeto.class.name.tableize}/#{objeto.id}/arriba", class: 'sb-link', title: 'Subir elemento', 'data-toggle' => 'tooltip', 'data-placement' => 'right' do %>
		<i class="bi bi-arrow-up-short"></i>
	<% end %>
<% end %>
<% unless objeto.orden == objeto.n_list %>
	<%= link_to "/#{objeto.class.name.tableize}/#{objeto.id}/abajo", class: 'sb-link', title: 'Bajar elemento', 'data-toggle' => 'tooltip', 'data-placement' => 'right' do %>
		<i class="bi bi-arrow-down-short"></i>
	<% end %>
<% end %>