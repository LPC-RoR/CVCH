<p id="notice"><%= notice %></p>

<h1>Propuestas</h1>

<table>
  <thead>
    <tr>
      <th>Instancia</th>
      <th>Publicacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @propuestas.each do |propuesta| %>
      <tr>
        <td><%= propuesta.instancia_id %></td>
        <td><%= propuesta.publicacion_id %></td>
        <td><%= link_to 'Show', propuesta %></td>
        <td><%= link_to 'Edit', edit_propuesta_path(propuesta) %></td>
        <td><%= link_to 'Destroy', propuesta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Propuesta', new_propuesta_path %>
