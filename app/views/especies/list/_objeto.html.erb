<small><b>
<i class="bi bi-tag"></i> 
<% unless objeto.filo_sinonimo.blank? %>
	[ <%= objeto.filo_sinonimo.filo_esp_sinos.map {|fes| fes.tipo}.uniq.join(' + ') %> ] 
<% end %>
<%= render layout: '0p/layout/tooltip_link', locals: {link: objeto, tooltip: 'Ver Etiqueta'} do %>
	<%= objeto.especie %>
<% end %>
<%#= link_to objeto.especie, objeto %></b> 
<% unless objeto.publicaciones.empty? %>
	<i class="bi bi-dot"></i> <%= objeto.publicaciones.count %>
<% end %>
</br>
<% unless objeto.filo_sinonimo.blank? %>
	<% objeto.filo_sinonimo.filo_esp_sinos.each do |rel| %>
		<div class="filo_especies_sinonimo" style="padding-left: 1em;">
			<i class="bi bi-arrow-right"></i> <i class="bi bi-bug"></i> 
			<%= render layout: '0p/layout/tooltip_link', locals: {link: "/publicos/especies?indice=#{rel.filo_especie.id}", tooltip: 'Ver Especie'} do %>
				<%= rel.filo_especie.filo_especie %>
			<% end %>
			<%#= link_to rel.filo_especie.filo_especie, "/publicos/especies?indice=#{rel.filo_especie.id}" %>
			</br>
		</div>
	<% end %>
<% end %>
</small>